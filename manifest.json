{
  "manifest_version": 3,
  "name": "PassCheck — Проверка пароля",
  "version": "1.5",
  "description": "Умная проверка пароля с zxcvbn, темами и анимациями.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "PassCheck"
  },
  "permissions": ["clipboardWrite", "storage"]
}
